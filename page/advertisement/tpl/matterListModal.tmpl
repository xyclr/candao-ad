<table class="table table-list">
    <thead>
        <tr> <th>选择</th> <th>素材ID</th> <th>素材名称</th><th>缩略图</th><th>素材URL</th><th>点击跳转(URL)</th></tr>
    </thead>
    <tbody>
        <% if (list.length>0) { %>
            <% _.forEach(list, function(item) { %>
                <tr><td><input type="radio" name="adMaterItem"></td><td><%- item.id %></td><td class="ad-matter-name"><%- item.name %></td><td><div class="img-thumb"><img class="J-btn-matter-view" src="<%- item.horizontalImg %>" onerror="this.src='/static/i/default.jpg'"></div></td><td><%- item.videoUrl %></td><td><%- item.webUrl %></td></tr>
            <% }); %>
        <% } else{ %>
            <tr><td colspan="6" style="padding:70px 0;">无素材,点击新建素材</td></tr>
        <% } %>
        </tr>
    </tbody>
</table>
<div class="f-cb"></div>